(defun solve (xs)
    (loop for ratio in xs
       with tak = 1
       with aoki = 1
       with n
       do
         (setq n (max (ceiling tak (first ratio))
                      (ceiling aoki (rest ratio))))
         (setq tak (* n (first ratio)))
         (setq aoki (* n (rest ratio)))
       finally
         (return (+ tak aoki))))


(defun main ()
  (declare #.OPT)
  (let* ((n (read))
         (xs (loop repeat n collect (cons (read) (read)))))
    (princ (solve xs))
    (fresh-line)))

